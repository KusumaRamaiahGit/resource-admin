    <servlet-class>controller.ReservationController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReservationController</servlet-name>
    <url-pattern>/ReservationController</url-pattern>
  </servlet-mapping>
  <servlet>
